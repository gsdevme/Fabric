{
  "name": "gsdev/fabric",
  "description": "Fabric is a set of interfaces, traits and simple classes to help structure libraries and code that interact with APIs.",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Gsdev\\Fabric\\": [
        "src/Fabric"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gsdev\\Fabric\\Test\\": [
        "tests/"
      ]
    }
  },
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0"
  },
  "suggest": {
    "guzzlehttp/guzzle": "If you want to use the Guzzle client"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^1.0",
    "phpunit/phpunit": "^7.0",
    "guzzlehttp/guzzle": "^6.0",
    "phpstan/phpstan-shim": "@stable",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "slevomat/coding-standard": "@stable",
    "mockery/mockery": "^1.0",
    "symfony/var-dumper": "^3.4|^4.0"
  }
}
